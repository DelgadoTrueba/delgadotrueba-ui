{
  "name": "@delgadotrueba/ui-web-components",
  "version": "0.0.1-SNAPSHOT",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/delgadotrueba-ui/delgadotrueba-ui.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DelgadoTrueba/delgadotrueba-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DelgadoTrueba/delgadotrueba-ui/issues"
  },
  "homepage": "https://github.com/DelgadoTrueba/delgadotrueba-ui#readme",
  "scripts": {
    "st:linter": "npx tsc --noEmit -p ./tsconfig.json",
    "sb:linter": "npx tsc --noEmit -p ./.storybook/tsconfig.json",
    "linter": "npx concurrently --kill-other -n \"ST:LINTER,SB:LINTER,ESLINT\" -c \"bgBlue.bold,bgGreen.bold,bgMagenta\" \"npm run st:linter\" \"npm run sb:linter\" \"npx eslint -c .eslintrc.js src/**/*.{ts,tsx}\"",
    "st:build": "stencil build --docs",
    "st:build:watch": "stencil build --docs-readme --watch --serve",
    "st:start": "stencil build --dev --watch --serve",
    "st:test": "stencil test --spec --e2e",
    "st:test.watch": "stencil test --spec --e2e --watchAll",
    "st:manifest": "npx cem analyze --stencil --globs 'src/**/*.tsx' --exclude 'src/**/*stories.tsx'",
    "sb:start": "start-storybook -p 6006 -s dist",
    "sb:build": "build-storybook",
    "start": "npx concurrently --kill-other -n \"STENCIL, STORYBOOK\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run st:manifest && npm run st:build:watch\" \"npx wait-on http://localhost:3333/ && npm run sb:start\""
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.19.0",
    "@babel/plugin-transform-typescript": "7.20.2",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@custom-elements-manifest/analyzer": "0.6.6",
    "@stencil/core": "2.19.3",
    "@stencil/eslint-plugin": "0.4.0",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-docs": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/cli": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/web-components": "6.5.14",
    "@types/jest": "27.0.3",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "babel-loader": "9.1.0",
    "concurrently": "7.6.0",
    "create-stencil": "3.1.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "husky": "8.0.2",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "jsx-dom": "8.0.3",
    "lint-staged": "13.1.0",
    "lit-html": "2.4.0",
    "prettier": "2.8.1",
    "puppeteer": "10.0.0",
    "storybook": "6.5.14",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.9.4",
    "wait-on": "6.0.1"
  },
  "customElements": "custom-elements.json"
}
