{
  "name": "delgadotrueba-ui",
  "version": "0.0.1-SNAPSHOT",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/delgadotrueba-ui/delgadotrueba-ui.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DelgadoTrueba/delgadotrueba-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DelgadoTrueba/delgadotrueba-ui/issues"
  },
  "homepage": "https://github.com/DelgadoTrueba/delgadotrueba-ui#readme",
  "scripts": {
    "st:build": "stencil build --docs",
    "st:build:watch": "stencil build --docs-readme --watch --serve",
    "st:start": "stencil build --dev --watch --serve",
    "st:test": "stencil test --spec --e2e",
    "st:test.watch": "stencil test --spec --e2e --watchAll",
    "sb:start": "start-storybook -p 6006 -s dist",
    "sb:build": "build-storybook",
    "start": "npx concurrently --kill-other -n \"STENCIL, STORYBOOK\" -c \"bgBlue.bold,bgGreen.bold,bgMagenta.bold\" \"npm run st:build:watch\" \"npx wait-on http://localhost:3333/ && npm run sb:start\""
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@stencil/core": "2.18.0",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/cli": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/web-components": "6.5.13",
    "@types/jest": "27.0.3",
    "babel-loader": "8.3.0",
    "concurrently": "7.5.0",
    "create-stencil": "3.1.0",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "lit-html": "2.4.0",
    "puppeteer": "10.0.0",
    "storybook": "6.5.13",
    "wait-on": "6.0.1"
  }
}
